{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "matchPackageNames": [
        "minio/mc"
      ],
      "versioning": "regex:^RELEASE\\.(?<major>\\d{4})-(?<minor>\\d{2})-(?<patch>\\d{2})T[0-9-]+Z$"
    }
  ],
  "extends": [
    ":automergeMinor"
  ]
}
